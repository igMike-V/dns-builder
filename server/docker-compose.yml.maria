version: '3'
volumes:
  data:
services:
  db:
    image: mariadb:10.4
    restart: always
    environment:
      MARIADB_ROOT_PASSWORD: root
      MARIADB_DATABASE: dns-helper
      MARIADB_USER: user
      MARIADB_PASSWORD: password
    volumes: 
      - data:/var/lib/mysql
    ports:
      - 3306:3306